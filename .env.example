# PostgreSQL Migration Tool - Environment Configuration

# Source Database
SOURCE_HOST=localhost
SOURCE_PORT=6000
SOURCE_DATABASE=source_db
SOURCE_USER=postgres
SOURCE_PASSWORD=your_password

# Destination Database
DEST_HOST=localhost
DEST_PORT=6000
DEST_DATABASE=dest_db
DEST_USER=postgres
DEST_PASSWORD=your_password

# Migration Settings
MIGRATION_BATCH_SIZE=1000
MIGRATION_LOG_LEVEL=INFO
MIGRATION_ENABLE_FK=true
MIGRATION_ENABLE_INDEXES=true
MIGRATION_TARGET_SCHEMA=migrated
